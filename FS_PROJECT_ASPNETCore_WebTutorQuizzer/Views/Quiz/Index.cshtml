@{
    ViewData["Title"] = "Show Quizzes";
}
@model IEnumerable<FS_PROJECT_ASPNETCore_WebTutorQuizzer.Models.QuizModel>

<h1>This is Quiz Page</h1>
<button class="btn btn-success">
<a href="@Url.Action("Create", "Quiz")" class="nav-link link-body-emphasis">
    Create Quiz
    </a>
</button>


<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Name</th>
             <th scope="col">Finished</th>
            <th scope="col">Subject</th>
             <th scope="col">Date</th>
        </tr>
    </thead>

    <tbody>
        @foreach(var quiz in Model)
        {
            <tr>
                <td>@quiz.Name</td>
                <td>@quiz.IsDone</td>
                <td>@quiz.SubjectId</td>
                <td>@quiz.DateTime</td>
            </tr>
        }
    </tbody>
</table>
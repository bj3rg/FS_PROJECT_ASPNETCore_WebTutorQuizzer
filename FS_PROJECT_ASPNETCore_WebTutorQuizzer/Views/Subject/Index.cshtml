@{
    ViewData["Title"] = "Show Subject";
}

@model IEnumerable<FS_PROJECT_ASPNETCore_WebTutorQuizzer.Models.SubjectModel>
<h1>Subject Page</h1>
<button class="btn btn-success">
    <a href="@Url.Action("Create", "Subject")" class="nav-link link-body-emphasis">
        Create Subject
    </a>
</button>

<table class="table">
<thead>
    <tr>
            <th>Id</th>
        <th>Title</th>
        
    </tr>
</thead>    
<tbody>
        @foreach(var subject in Model)
        {
    
            <tr>
                <td>@subject.Id</td>
                <td>@subject.Title</td>
            </tr>
        }
    </tbody>

</table>



@{
}
